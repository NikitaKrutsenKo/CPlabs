@model Hospital

<h1>Hospital Details</h1>

<table class="table">
    <tr>
        <th>Hospital Name</th>
        <td>@Model.HospitalName</td>
    </tr>
    <tr>
        <th>Address</th>
        <td>@Model.Address.Line1, @Model.Address.City, @Model.Address.StateProvince</td>
    </tr>
    <tr>
        <th>Other Details</th>
        <td>@Model.OtherDetails</td>
    </tr>
</table>

